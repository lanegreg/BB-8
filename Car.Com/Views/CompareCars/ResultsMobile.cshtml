  <div id="customize_comparison_overlay_js" class="overlay overlay-dk" style="display:none">
    <button id="close_customize_compare_js" class="close_btn_js overlay-close">
      <span class="icon">
        <svg>
          <use xlink:href="/assets/svg/global-sprite.svg#i_close"></use>
        </svg>
      </span>
    </button>
    <h1 class="h1">Customize My Comparison</h1>

    <div id="compareCustomizeMain" class="compare-cust-cont">
    </div>


  </div>

</div>

